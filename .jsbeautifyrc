{
   "brace_style": "none,preserve-inline",
   "indent_size": 4,
   "indent_char": " ",
   "jslint_happy": true,
   "unformatted": ["a","pre"],
   "css": {
     "indent_size": 4
   },
    "beautify.language": {
    "js": {
      "type": ["javascript", "json"],
      "filename": [".jshintrc", ".jsbeautifyrc"]
    },
    "css": ["css", "scss"],
    "html": ["htm", "html"]
  },
  "end_with_newline": false,
  "unescape_strings": false,
  "space_around_combinator": false
 }